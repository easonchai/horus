# Coinbase Developer Platform API credentials
CDP_API_KEY_NAME= #name or id of the api key
CDP_API_KEY_PRIVATE_KEY= #private key of the api key

# OpenAI API (Required for real OpenAI analysis)
OPENAI_API_KEY= #api key for openai

# Optional: Network configuration (defaults to Base Sepolia testnet)
# CDP_NETWORK=base-sepolia 

# Twitter API (Optional if USE_MOCK_DATA=true)
TWITTER_BEARER_TOKEN= #bearer token for Twitter API v2
TWITTER_API_KEY= #consumer key for Twitter API
TWITTER_API_SECRET= #consumer secret for Twitter API
TWITTER_ACCESS_TOKEN= #access token for Twitter API
TWITTER_ACCESS_TOKEN_SECRET= #access token secret for Twitter API

# Horus Configuration
USE_MOCK_DATA=true  # Set to true to use mock Twitter data, false to use real Twitter API

# Operational settings (optional)
TWITTER_CHECK_INTERVAL=300  # Seconds between Twitter checks